{
  "name": "ipc-electron-react",
  "version": "0.1.0",
  "description": "test-server-ipc-electron-react",
  "private": true,
  "author": "mike",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "chokidar": "^3.5.3",
    "custom-electron-titlebar": "^4.1.0",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.2.1",
    "express": "^4.18.1",
    "is-reachable": "^5.2.1",
    "node-fetch": "^3.2.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tcp-port-used": "^1.0.2",
    "web-vitals": "^2.1.4"
  },
  "main": "electron/main.js",
  "scripts": {
    "start:client": "set PORT=3010 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "electron .",
    "build-electron": "react-scripts build",
    "electron:build": "yarn build-electron && electron-builder build --publish never",
    "start:server": "nodemon ./backend/index.js",
    "start:generate": "node electron/generate.js"
  },
  "homepage": "./",
  "build": {
    "appId": "com.app.id",
    "productName": "Ipc Server Electron React",
    "files": [
      "build/**/*",
      "electron/main.js",
      "electron/mock",
      "electron/preload.js",
      "node_modules/**/*",
      "backend/express.js"
    ],
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0",
    "nodemon": "^2.0.19"
  },
  "keywords": [],
  "license": "ISC"
}
